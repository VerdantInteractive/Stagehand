[configuration]

entry_symbol = "stagehand_library_init"
compatibility_minimum = "4.6"
reloadable = true

[libraries]

macos.debug = "res://addons/stagehand/bin/libstagehand.macos.template_debug.framework"
macos.release = "res://addons/stagehand/bin/libstagehand.macos.template_release.framework"
ios.debug = "res://addons/stagehand/bin/libstagehand.ios.template_debug.xcframework"
ios.release = "res://addons/stagehand/bin/libstagehand.ios.template_release.xcframework"
windows.debug.x86_32 = "res://addons/stagehand/bin/libstagehand.windows.template_debug.x86_32.dll"
windows.release.x86_32 = "res://addons/stagehand/bin/libstagehand.windows.template_release.x86_32.dll"
windows.debug.x86_64 = "res://addons/stagehand/bin/libstagehand.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "res://addons/stagehand/bin/libstagehand.windows.template_release.x86_64.dll"
linux.debug.x86_64 = "res://addons/stagehand/bin/libstagehand.linux.template_debug.x86_64.so"
linux.release.x86_64 = "res://addons/stagehand/bin/libstagehand.linux.template_release.x86_64.so"
linux.debug.arm64 = "res://addons/stagehand/bin/libstagehand.linux.template_debug.arm64.so"
