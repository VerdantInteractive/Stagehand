android.debug.arm64 = "res://addons/stagehand/bin/libstagehand.android.template_debug.arm64.so"
android.release.arm64 = "res://addons/stagehand/bin/libstagehand.android.template_release.arm64.so"
web.debug = "res://addons/stagehand/bin/libstagehand.web.template_debug.wasm32.wasm"
web.release = "res://addons/stagehand/bin/libstagehand.web.template_release.wasm32.wasm"

[dependencies]
ios.debug = {
    "res://addons/stagehand/bin/libgodot-cpp.ios.template_debug.xcframework": ""
}
ios.release = {
    "res://addons/stagehand/bin/libgodot-cpp.ios.template_release.xcframework": ""
